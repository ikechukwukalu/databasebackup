{
    "name": "ikechukwukalu/databasebackup",
    "description": "A laravel package for backing up database with local and remote connection.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Ikechukwukalu\\Databasebackup\\": "src/",
            "Ikechukwukalu\\Databasebackup\\Console\\Commands\\": "src/Console/Commands/",
            "Ikechukwukalu\\Databasebackup\\Console\\": "src/Console/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ikechukwukalu\\Databasebackup\\Tests\\": "Tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "authors": [
        {
            "name": "Ikechukwu Kalu",
            "email": "ea.ikechukwukalu@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "illuminate/console": "^8.0|^9.0|^10.0",
        "illuminate/database": "^8.0|^9.0|^10.0",
        "illuminate/http": "^8.0|^9.0|^10.0",
        "illuminate/routing": "^8.0|^9.0|^10.0",
        "illuminate/support": "^8.0|^9.0|^10.0",
        "php": ">=7.3"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0|^8.0",
        "phpunit/phpunit": "^9.0|^10.0",
        "php-parallel-lint/php-parallel-lint": "dev-develop"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ikechukwukalu\\Databasebackup\\DatabaseBackupServiceProvider"
            ]
        }
    }
}
